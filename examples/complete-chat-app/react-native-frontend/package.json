{
  "name": "chat-app-frontend",
  "version": "1.0.0",
  "description": "React Native frontend for the ActionCable chat app",
  "main": "expo-router/entry",
  "scripts": {
    "start": "npx expo start --dev-client --clear",
    "start:android": "npx expo start --android --dev-client --clear",
    "start:ios": "npx expo start --ios --dev-client --clear",
    "start:web": "npx expo start --web --clear",
    "lint": "npx expo lint",
    "fresh": "yarn start --reset-cache",
    "installDevBuild:ios": "npx expo run:ios",
    "installDevBuild:android": "npx expo run:android",
    "rebuildNativeDeps": "npx expo prebuild",
    "rebuildNativeDeps:clean": "npx expo prebuild --clean",
    "freshAndroid": "npx expo prebuild --clean && npx expo run:android",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@kesha-antonov/react-native-action-cable": "link:../../..",
    "expo": "~54.0.0",
    "expo-clipboard": "~8.0.0",
    "expo-constants": "~18.0.0",
    "expo-linking": "~8.0.0",
    "expo-router": "~5.0.0",
    "expo-status-bar": "~3.0.0",
    "expo-system-ui": "^6.0.9",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "4.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.0",
    "@types/react": "~19.0.0",
    "typescript": "~5.9.0"
  },
  "private": true
}
